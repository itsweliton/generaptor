{
  "name": "generaptor",
  "version": "0.1.7",
  "description": "A React library to generate dynamic forms with material-ui at the speed of a Velociraptor ðŸ¦–",
  "main": "dist/formGenerator.min.js",
  "style": "dist/formGenerator.min.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welitonderesende/generaptor.git"
  },
  "keywords": [
    "react",
    "forms",
    "form-generator"
  ],
  "author": "Weliton de Resende <welitonderesende@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/welitonderesende/generaptor/issues"
  },
  "homepage": "https://github.com/welitonderesende/generaptor#readme",
  "dependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.4.2",
    "@material-ui/pickers": "^3.2.6",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.0.2",
    "calidators": "^3.0.2",
    "css-loader": "2.1.1",
    "date-fns": "^2.0.0-beta.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "24.8.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.9.0",
    "style-loader": "1.0.0",
    "use-deep-compare-effect": "^1.2.0",
    "webpack": "^4.41.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^3.2.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack",
    "test": "jest -u --coverage",
    "dev": "webpack-dev-server --mode=development"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
